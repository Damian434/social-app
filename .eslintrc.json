{
  "env": {
      "browser": true,
      "es6": true
  },
  "extends": [
      "standard"
  ],
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
  },
  "rules": {
    "eol-last": "off",
    "semi": "off",
    "no-unused-vars":"warn",
    "object-property-newline": "warn"
  }
}